CC       =cl
CXX      =cl
LD       =link
AR       =CHANGE_ME

CFLAGS   =-EHsc -c -O2 -Ot -Ox -D__STDC__ -DHAVE_MALLOC=1 -DHAVE_REALLOC=1\
 -DADOLC_DLL=1 -DADOLC_VERSION=1 -DADOLC_SUBVERSION=9 -DADOLC_PATCHLEVEL=0\
 -nologo
CXXFLAGS =-EHsc -c -O2 -Ot -Ox -D__STDC__ -DHAVE_MALLOC=1 -DHAVE_REALLOC=1\
 -DADOLC_DLL=1 -DADOLC_VERSION=1 -DADOLC_SUBVERSION=9 -DADOLC_PATCHLEVEL=0\
 -nologo
DLLFLAGS =-dll -version:0.0 -out:adolc.dll
LDFLAGS  =-nologo
ARFLAGS  =CHANGE_ME
